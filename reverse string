class Solution {
    public void reverseString(char[] s) {
        int left = 0, right = s.length - 1;
        while (left < right) {
            char tmp = s[left];
            s[left++] = s[right];
            s[right--] = tmp;
        }
    }
}


for(int i=0; i<s.size()/2; i++) swap(s[i],s[s.size()-i-1]);
// or
reverse(s.begin(), s.end());
